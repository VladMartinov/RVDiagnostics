<Application x:Class="RVDiagnostics.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RVDiagnostics"
             StartupUri="Views/SplashScreen.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Buttons.xaml" />
                <ResourceDictionary Source="Resources/Themes/Dark.xaml"/>
                <ResourceDictionary Source="Resources/Localization/Strings.ru.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>

    </Application.Resources>
</Application>
